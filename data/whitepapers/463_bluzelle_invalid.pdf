          <div class="grid-container"></div>
          <script>
  const gridSize = 40;
  const dotSize = 5;
  const spreadDistance = 40;
  const gridContainer = document.querySelector('.grid-container');
  const dotBlock = document.querySelector('.dot-block');
  const dots = [];
  const containerRect = dotBlock.getBoundingClientRect();
  const containerWidth = containerRect.width;
  const containerHeight = containerRect.height;
  for (let x = gridSize; x < containerWidth; x += gridSize) {
    for (let y = gridSize; y < containerHeight; y += gridSize) {
      const dot = document.createElement('div');
      dot.className = 'dot';
      dot.style.width = `${dotSize}px`;
      dot.style.height = `${dotSize}px`;
      dot.style.left = `${x}px`;
      dot.style.top = `${y}px`;
      gridContainer.appendChild(dot);
      dots.push({ element: dot, x, y });
    }
  }
  gridContainer.addEventListener('mousemove', (event) => {
    // Update the containerRect variable each time the mouse moves.
    const containerRect = gridContainer.getBoundingClientRect();
    const mouseX = event.clientX - containerRect.left;
    const mouseY = event.clientY - containerRect.top;
    dots.forEach((dot) => {
      const dx = dot.x - mouseX;
      const dy = dot.y - mouseY;
      const distance = Math.sqrt(dx * dx + dy * dy);
      const spreadFactor = Math.min(spreadDistance / distance, 1);
      dot.element.style.transform = `translate(${dx * spreadFactor}px, ${dy * spreadFactor}px)`;
    });
  });
  gridContainer.addEventListener('mouseleave', () => {
    dots.forEach((dot) => {
      dot.element.style.transform = 'translate(0, 0)';
    });
  });
</script>
